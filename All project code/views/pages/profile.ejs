<main>
  <div class="container">
    <h1><center>Welcome User: <%= user.username %></center></h1>
    <table class="table">
      <tbody>
        <tr>
          <td>FIRST NAME</td>
          <td><%= user.first_name %></td>
        </tr>
        <tr>
          <td>LAST NAME</td>
          <td><%= user.last_name %></td>
        </tr>
        <tr>
          <td>EMAIL</td>
          <td><%= user.email %></td>
        </tr>
        <tr>
          <td>YEAR</td>
          <td><%= user.year %></td>
        </tr>
        <tr>
          <td>MAJOR</td>
          <td><%= user.major %></td>
        </tr>
        <tr>
          <td>DEGREE</td>
          <td><%= user.degree %></td>
        </tr>
      </tbody>
    </table>

    <h2>Favorite Books</h2>
    <ul>
      <% books.forEach(function(book) { %>
        <li><%= book.title %> by <%= book.author %></li>
      <% }) %>
    </ul>

    <h2>Annotations</h2>
    <% annotations.forEach(function(annotation) { %>
      <h4><%= annotation.title %> - Page <%= annotation.page_number %></h4>
      <p><%= annotation.comment %></p>
    <% }) %>
  </div>
</main>
